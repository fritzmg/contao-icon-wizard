{
    "name":"netzmacht/contao-iconwizard",
    "description":"Provides an backend widget for choosing icons of an icon font",
    "keywords":["icons", "widget", "wizard", "contao", "dca"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Project leader"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/iconWizard/issues",
        "wiki": "http://netzmacht.de/erweiterungen/entwickler-tools/icon-wizard/",
        "source":"https://github.com/netzmacht/iconWizard"
    },
    "require":{
        "php":">=5.3",
        "contao/core":">=3.0"
    },
    "autoload":{
        "classmap":["src/system/modules/iconWizard/"]
    },
    "suggest": {
        "netzmacht/font-awesome" : "Use Font Awesome icons"
    },
    "replace": {
        "contao-legacy/iconWizard" : "*"
    },
    "extra":{
        "contao": {
            "symlinks":{
                "src/system/modules/iconWizard": "system/modules/iconWizard"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}